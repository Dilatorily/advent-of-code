{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "paths": {
      "#dilatorily/advent-of-code/*": ["./src/*.ts"]
    },
    "strict": true,
    "target": "ESNext"
  },
  "exclude": ["node_modules", "src/2015", "src/2021", "src/2022", "src/2023"],
  "include": ["*.config.ts", "src"]
}
