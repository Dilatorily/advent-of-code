{
  "compilerOptions": {
    "erasableSyntaxOnly": true,
    "isolatedModules": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noEmit": true,
    "paths": {
      "#dilatorily/advent-of-code/*": ["./src/*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  },
  "exclude": ["node_modules"],
  "include": ["*.config.ts", "src/**/*.ts"]
}
